<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.library.bean.Borrow_Return_Book" table="borrow_return_book">
		<id name="ID">
			<generator class="increment"></generator>
		</id>
			<property name="bID"></property>
			<property name="uID"></property>
			<property name="borrowDate"></property>
			<property name="backDate"></property>
			<property name="state"></property>
			<property name="returnDate"></property>
	</class>
</hibernate-mapping>